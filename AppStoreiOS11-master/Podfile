  pod 'Kingfisher', '~> 3.0'
  pod 'Cosmos', '~> 10.0'
  pod 'ReadMoreTextView'
  pod 'AZSearchView'
  pod 'PFWebViewController', '~> 1.1.1'
  pod 'BMPlayer', '~> 0.9.1'
end

